  "scripts": {
    "start": "craco start",
    "start:mock": "REACT_APP_MOCKED=1 craco start",
    "postinstall": "cp .env .env.local",
    "build": "craco build",
    "test": "REACT_APP_CHAIN=0x1 craco test",
    "lint:es": "eslint . --ext .ts,.tsx,.js --max-warnings 0",
    "eject": "react-scripts eject",
    "generate": "env-cmd yarn apollo codegen:generate --target=typescript --globalTypesFile=src/__generated__/globalTypes.ts"
  },
  "eslintConfig": {
    "plugins": [
      "simple-import-sort"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "simple-import-sort/imports": "warn",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.18.3",
    "@types/lodash": "^4.14.171",
